      "id": "year.month.json.get",
      "title": "year.month.json.get",
      "description": "The Archive API provides lists of NYT articles by month going back to 1851.  Simply pass in the year and month you want and it returns a JSON object with all articles for that month.\n",
      "inputSchema": {
        "type": "object",
        "properties": {
          "year": {
            "type": "integer",
            "format": "int32",
            "description": "The year (e.g. 2016).",
            "maximum": 2030,
            "minimum": 1851,
            "default": 2016
          },
          "month": {
            "type": "integer",
            "format": "int32",
            "description": "The month number (e.g. 1 for January).",
            "maximum": 12,
            "minimum": 1,
            "default": 1
          }
        },
        "additionalProperties": false,
        "required": [
          "year",
          "month"
        ]
      },
      "outputSchema": {
        "properties": {
          "response": {
            "properties": {
              "docs": {
                "items": {
                  "$ref": "#/definitions/Doc"
                },
                "type": "array"
              },
              "meta": {
                "properties": {
                  "hits": {
                    "type": "integer"
                  },
                  "offset": {
                    "type": "integer"
                  },
                  "time": {
                    "type": "integer"
                  }
                },
                "type": "object"
              }
            },
            "type": "object"
          }
        },
        "type": "object"
      },
