        "source": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "url": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "ArticleWithCountType": {
      "properties": {
        "abstract": {
          "type": "string"
        },
        "byline": {
          "type": "string"
        },
        "column": {
          "type": "string"
        },
        "count_type": {
          "type": "string"
        },
        "des_facet": {
          "$ref": "#/definitions/DesFacet"
        },
        "geo_facet": {
          "$ref": "#/definitions/GeoFacet"
        },
        "media": {
          "items": {
            "properties": {
              "caption": {
                "type": "string"
              },
              "copyright": {
                "type": "string"
              },
